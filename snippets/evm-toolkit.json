{
  ".source.evm-toolkit": {
    "push macro": {
      "prefix": "%push",
      "body": "%push(${1:number})"
    },
    "custom expression macros": {
      "prefix": "%def",
      "body": ["%def ${1:func_name}(${2:input})", "\t$0", "%end"]
    },
    "custom instruction macros": {
      "prefix": "%macro",
      "body": ["%macro ${1:func_name}(${2:input})", "\t$0", "%end"]
    },
    "import macro": {
      "prefix": "%import",
      "body": "%import(\"${1:name}\")"
    },
    "include macro": {
      "prefix": "%include",
      "body": "%include(\"${1:name}\")"
    },
    "include_hex macro": {
      "prefix": "%include_hex",
      "body": "%include_hex(\"${1:name}\")"
    },
    "selector macro": {
      "prefix": "selector",
      "body": "selector(\"${1:function_type}\")"
    },
    "topic macro": {
      "prefix": "topic",
      "body": "topic(\"${1:log_type}\")"
    },
    "constructor bootstrap": {
      "prefix": "constructor bootstrap",
      "body": [
        "# put runtime length on the stack",
        "%push(end_runtime - start_runtime)\n",
        "# copy runtime to memory",
        "dup1",
        "%push(start_runtime)",
        "returndatasize",
        "codecopy\n",
        "# return runtime bytecode",
        "returndatasize",
        "return\n",
        "start_runtime:",
        "# --- RUNTIME CODE ---",
        "$0\n",
        "end_runtime:"
      ]
    },
